package com.nari.baseapp.datagatherman;

import javax.persistence.Column;
import javax.persistence.Id;
import javax.persistence.Table;

/*****
 * 后台任务明细
 * 
 * @author huangxuan *
 ****/
@Table(name = "t_bg_task_detail")
public class TbgTaskDetail {
	@Id
	@Column(name = "task_id")
	private Integer taskId;
	@Column(name = "data_item_no")
	private String dataItemNo;
	@Column(name = "data_type")
	private Integer dataType;

	public Integer getTaskId() {
		return taskId;
	}

	public void setTaskId(Integer taskId) {
		this.taskId = taskId;
	}

	public String getDataItemNo() {
		return dataItemNo;
	}

	public void setDataItemNo(String dataItemNo) {
		this.dataItemNo = dataItemNo;
	}

	public Integer getDataType() {
		return dataType;
	}

	public void setDataType(Integer dataType) {
		this.dataType = dataType;
	}
}
